    /** Estructura que contiene el avalúo del predio en la base de datos catastral.
     */
    STRUCTURE ILC_EstructuraAvaluo =
      /** Fecha de la vigencia del avaluo catastral.
       */
      Fecha_Avaluo_Catastral : MANDATORY INTERLIS.XMLDate;
      /** Este valor es el derivado de los procesos de formación y actualización de la gestión catastral como el resultado de la investigación económica, las normas urbanísticas municipales, el análisis del mercado inmobiliario y los costos de la construcción.
       * 
       * El valor comercial corresponde a la suma del valor comercial de las unidades de construcción más el valor comercial total del terreno. Para algunos casos de Propiedad Horizontal PH, se obtiene un valor integral. Este valor debe ser mayor a cero (0).
       * 
       * Este valor comercial permanecerá asociado para todos los predios públicos y privados en la base de datos catastral, hasta que se realice un posterior proceso catastral que lo modifique.
       */
      Valor_Comercial : LADM_COL_V3_1.LADM_Nucleo.Peso;
      /** Es el valor de un predio, resultante de un ejercicio técnico que, en ningún caso, podrá ser inferior al 60% del valor comercial o superar el valor de este último. Para su determinación no será necesario calcular de manera separada el valor del suelo y el de la construcción.
       */
      Avaluo_Catastral : MANDATORY LADM_COL_V3_1.LADM_Nucleo.Peso;
      /** Este valor es el derivado de los procesos de formación y actualización de la gestión catastral y es resultado de la investigación económica, las normas urbanísticas municipales y el análisis del mercado inmobiliario.
       */
      Valor_Comercial_Terreno : LADM_COL_V3_1.LADM_Nucleo.Peso;
      /** Es el valor del terreno, resultante de un ejercicio técnico que, en ningún caso, podrá ser inferior al 60% del valor comercial del terreno o superar el valor de este último.
       */
      Avaluo_Catastral_Terreno : LADM_COL_V3_1.LADM_Nucleo.Peso;
      /** Este valor es el derivado de los procesos de formación y actualización de la gestión catastral y es resultado de la investigación económica, las normas urbanísticas municipales y el análisis del mercado inmobiliario de la suma total de las unidades de construcción.
       */
      Valor_Comercial_Total_UnidadesConstruccion : LADM_COL_V3_1.LADM_Nucleo.Peso;
      /** Es el valor de la suma total de las unidades de construcción, resultante de un ejercicio técnico que, en ningún caso, podrá ser inferior al 60% del valor comercial de la suma total de las unidades de construcción o superar el valor de este último.
       */
      Avaluo_Catastral_Total_UnidadesConstruccion : LADM_COL_V3_1.LADM_Nucleo.Peso;
      /** Es el derecho que tiene el propietario, poseedor u ocupante de predios de presentar antes del 30 de junio de cada año ante el gestor catastral, la autoestimación del avalúo catastral. En los municipios donde no hubiere oficina de catastro o donde el gestor no tenga presencia, su presentación se hará ante la Secretaría de Hacienda o quien haga sus veces en el respectivo municipio o a través de los canales virtuales dispuestos por cada gestor.
       * 
       * Dicha autoestimación no podrá ser inferior al avalúo catastral vigente y se incorporará al catastro con fecha 31 de diciembre del año en el cual se haya efectuado, si el gestor catastral la encuentra justificada mediante las pruebas que aporte de las mutaciones físicas, valorización, cambios de uso o mercado inmobiliario.
       * 
       * La autoestimación aquí definida se refiere a las disposiciones contenidas en la Ley 14 de 1983.
       */
      !!@ No="0 "
      !!@ MetaAttribute="1 "
      Autoestimacion : MANDATORY BOOLEAN;
      id_operacion_predio : TEXT*255;
    END ILC_EstructuraAvaluo;

    /** Estructura que permite identificar las modificaciones que puede tener el número predial producto de la dinámica inmobiliaria identificada en el levantamiento catastral.
     */
    STRUCTURE ILC_EstructuraNovedadNumeroPredial =
      /** El predio no sufre modificación del código predial nacional de 30 dígitos de acuerdo con la estructura.
       */
      Numero_Predial : MANDATORY TEXT*30;
      /** Indica los diferentes tipos de modificaciones que puede tener el número predial producto de la dinámica inmobiliaria identificada en el levantamiento catastral.
       */
      Tipo_Novedad : MANDATORY (
        /** Al predio matriz y sus segregados se asigna esta novedad cuando se presenta desenglobe donde el predio matriz mantiene FMI con estado "Activo" y área de terreno remanente.
         */
        Desenglobe_Venta_Parcial,
        /** Se asigna esta novedad a los predios segregados como resultado de un desenglobe por división material.
         */
        Desenglobe_Division_Material,
        /** Se asigna esta novedad al predio con "numeración provisional" que se incorpora en la base catastral, relacionando el FMI "Activo" en una agregación (englobe).
         */
        Englobe_Nuevo_FMI,
        /** Se asigna esta novedad al predio que mantiene el FMI con estado "Activo" en un agregación (englobe).
         */
        Englobe_Mantiene_FMI,
        /** Se asigna esta novedad a los predios que deben ser cancelados en la base catastral y que no corresponden a englobes, desenglobes o cambios de numero predial.
         */
        Cancelacion,
        /** Se asigna esta novedad a los predios que forman parte de una agregación (englobe) y deben ser cancelados en la base catastral.
         */
        Cancelacion_por_Englobe,
        /** Se asigna esta novedad al predio matriz  del cual se segregan dos o más predios y que por ende debe ser cancelado en la base catastral.
         */
        Cancelacion_por_Desenglobe,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "Mejora" a "Informal"
         */
        Cambio_Numero_Predial_Mejora_a_Informal,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "Mejora" a "NPH"
         */
        Cambio_Numero_Predial_Mejora_a_NPH,
        /** Se asigna esta novedad al predio que cambia  su "Numero Predial Nacional" al pasar de "Condicion_predio" de "NPH" a "Informal"
         */
        Cambio_Numero_Predial_NPH_a_Informal,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "NPH" a "Condominio"
         */
        Cambio_Numero_Predial_NPH_a_Condominio,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "NPH" a "PH"
         */
        Cambio_Numero_Predial_NPH_a_PH,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "NPH" a "Via"
         */
        Cambio_Numero_Predial_NPH_a_VIA,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "NPH" a "Bien_Uso_Publico"
         */
        Cambio_Numero_Predial_NPH_a_Bien_Uso_Publico,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "PH" a "Condominio"
         */
        Cambio_Numero_Predial_PH_a_Condominio,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de  "Condominio" a "PH"
         */
        Cambio_Numero_Predial_Condominio_a_PH,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de  "Condominio" a "NPH"
         */
        Cambio_Numero_Predial_Condominio_a_NPH,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de estar inscrito catastralmente en la zona urbana o corregimiento, a la zona rural.
         */
        Cambio_Numero_Predial_Urbano_a_Rural,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de estar inscrito catastralmente en la zona rural, a la zona urbana o corregimiento
         */
        Cambio_Numero_Predial_Rural_a_Urbano,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de estar inscrito en una manzana catastral a otra, dentro de la zona urbana o corregimiento
         */
        Cambio_Numero_Predial_Manzana_a_Manzana,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de estar inscrito en una vereda catastral a otra, dentro de la zona rural
         */
        Cambio_Numero_Predial_Vereda_a_Vereda,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de estar inscrito en una municipio a otro.
         */
        Cambio_Numero_Predial_Municipio_a_Municipio,
        /** Se asigna esta novedad a los predios con "numero provisional" que se incluyen en la base catastral y que no se originan a partir de un englobe, desenglobe o cambio de numero predial.
         */
        Predio_Nuevo
      );
      id_operacion_predio : TEXT*255;
    END ILC_EstructuraNovedadNumeroPredial;

    /** Clase que almacena las fuentes administrativas (escrituras, sentencias, actos administrativos, etc) que sustentan el Derecho respecto de la relación de tenencia entre el Interesado y el predio.
     */
    CLASS ILC_FuenteAdministrativa
    EXTENDS LADM_COL_V3_1.LADM_Nucleo.COL_FuenteAdministrativa =
      id_operacion_predio : TEXT*255;
      /** Documentos como escritura pública, sentencia judicial, acto administrativo, documento privado o sin documento (puede ser una declaración verbal), que acredita la adquisición de derechos de dominio sobre un bien inmueble y la relación que existe entre el interesado y el predio.
       */
      Tipo (EXTENDED) : MANDATORY Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.ILC_FuenteAdministrativaTipo;
      /** Entidades encargadas de emitir el título de dominio, posesión u ocupación.
       */
      Ente_Emisor : TEXT*255;
      Observacion : MTEXT*255;
      Numero_Fuente : NUMERIC;
      Estado_Disponibilidad : TEXT*255;
      Tipo_Principal : TEXT*255;
      Fecha_Documento_Fuente : INTERLIS.XMLDate;
    END ILC_FuenteAdministrativa;

    CLASS ILC_FuentesEspacialesEtnicas =
      Tipo : MANDATORY Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.ILC_FuentesEspacialesEtnicasTipo;
      Ente_Emisor : TEXT*20;
    END ILC_FuentesEspacialesEtnicas;

    CLASS ILC_IdentificacionEtnica =
      Nombre_Pueblo : MANDATORY Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.ILC_NombrePueblosIndigenasTipo;
      Nombre_Comunidad : TEXT*20;
    END ILC_IdentificacionEtnica;

    /** Es la parte que tiene una relación con el predio por algún tipo de derecho, restricción, publicidad o responsabilidad.
     */
    CLASS ILC_Interesado
    EXTENDS LADM_COL_V3_1.LADM_Nucleo.COL_Interesado =
      id_operacion_predio : TEXT*255;
      /** Son considerados dos tipos de interesados, persona natural que actúa a nombre propio y se ocupa de algunas actividades que la ley considera mercantiles y persona jurídica que actúa como institución creada por una o más personas para cumplir un objetivo social que puede ser con o sin fines de lucro.
       */
      Tipo : MANDATORY Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.CR_InteresadoTipo;
      /** Clase de documentos públicos que permite la identificación del interesado.
       */
      Tipo_Documento : MANDATORY Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.CR_DocumentoTipo;
      /** Documento público que contiene datos de identificación personal y que caracteriza al interesado como ciudadano, relacionado mediante un número de identificación.
       */
      Documento_Identidad : MANDATORY TEXT*50;
      /** Primer nombre de la persona interesada en trámite o proceso.
       */
      Primer_Nombre : TEXT*100;
      /** Segundo nombre de la persona interesada en trámite o proceso.
       */
      Segundo_Nombre : TEXT*100;
      /** Primer apellido de la persona interesada en trámite o proceso.
       */
      Primer_Apellido : TEXT*100;
      /** Segundo apellido de la persona interesada en trámite o proceso.
       */
      Segundo_Apellido : TEXT*100;
      /** Corresponde a “Masculino”, “Femenino” o "No Binario" según como aparezca en el tipo de documento de identidad. La identificación del sexo es obligatoria para las personas naturales. En el evento que no sea posible identificar el sexo de la persona natural se debe asignar el dominio "Sin Determinar". Para las personas juridicas no se debe identificar este atributo.
       */
      Sexo : Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.CR_SexoTipo;
      /** Grupo de personas a la cual pertenece el interesado, quienes comparten una misma ideología cultural.
       */
      Grupo_Etnico : Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.ILC_AutorreconocimientoEtnicoTipo;
      AutorreconocimientoCampesino : MANDATORY BOOLEAN;
      /** Nombre jurídico que recibe una sociedad colectiva o empresa constituida legalmente, con el cual se encuentra inscrito.
       */
      Razon_Social : TEXT*255;
      Nombre : TEXT*255;
      Porcentaje_Propiedad : TEXT*255;
      Telefono : TEXT*255;
      Autoriza_Notificacion_Correo : BOOLEAN;
      Correo_Electronico : TEXT*255;
      Direccion_Residencia : TEXT*255;
      Domicilio_Notificacion : TEXT*255;
      Departamento : TEXT*255;
      Municipio : TEXT*255;
      UNIQUE Documento_Identidad;
    END ILC_Interesado;

    /** Datos de contacto de la parte interesada.
     */
    CLASS ILC_InteresadoContacto =
      /** Número de teléfono de contacto del interesado.
       */
      Telefono : 3000000000 .. 6999999999;
      /** Domicilio para notificaciones del interesado.
       */
      Domicilio_Notificacion : TEXT*250;
      /** Dirección de residencia del interesado.
       */
      Direccion_Residencia : TEXT*250;
      /** Correo electrónico del interesado.
       */
      Correo_Electronico : TEXT*100;
      /** Indica si el interesado autoriza notificación vía correo electrónico.
       */
      Autoriza_Notificacion_Correo : MANDATORY BOOLEAN;
      /** Departamento de residencia del interesado.
       */
      Departamento : MANDATORY TEXT*100;
      /** Municipio de residencia del interesado.
       */
      Municipio : MANDATORY TEXT*100;
    END ILC_InteresadoContacto;

    /** Estructura que permite identificar las inconsistencias que puede tener el folio de matrícula inmobiliaria producto de la dinámica inmobiliaria identificada en el levantamiento catastral.
     */
    STRUCTURE ILC_NovedadFMI =
      /** Es el consecutivo que se asigna a cada oficina de registro de instrumentos públicos.
       */
      Codigo_ORIP : MANDATORY TEXT*4;
      /** Es el consecutivo que se asigna a cada predio jurídico abierto en la ORIP.
       */
      Numero_FMI : MANDATORY TEXT*80;
      /** Relaciona todas aquellas diferencias e inexactitudes relativas al registro de los predios respecto de la información jurídica vigente.
       */
      Tipo_NovedadFMI : (
        /** Cuando el estado del FMI del predio en estudio es CERRADO ante la SNR, pero pese a haberse realizado varios negocios jurídicos sobre un predio (ventas, donaciones, segregaciones, etc.) se identifica que este posee un área de remanente, lo que le permite continuar con su existencia jurídica y su estado debe cambiar a ABIERTO.
         */
        FMI_Debe_Estar_Abierto,
        /** Cuando el estado del FMI del predio en estudio es ABIERTO ante la SNR, pero el folio ha sido cancelado por orden de autoridad  judicial y/o administrativa, o se agote su área por englobes, desenglobes o cualquier otro negocio jurídico  y su estado debe cambiar a CERRADO.
         */
        FMI_Debe_Estar_Cerrado,
        /** Cuando no exista relación entre el predio segregado y el que se enuncia.
         */
        FMI_No_Es_Segregado_Del_Que_Se_Enuncia,
        /** Cuando se ha aperturado dos o mas FMI sobre un mismo predio, en un mismo circulo registral o círculos registrales diferentes.
         */
        FMI_Duplicado_Sobre_El_Mismo_Bien_Inmueble,
        /** Cuando se presente una inconsistencia en el código registral con el que se registró el acto jurídico
         */
        FMI_Con_Error_En_La_Identificacion_De_Naturaleza_Juridica_Del_Acto,
        /** Cuando se encuentra un titulo inscrito que no corresponde al predio objeto de estudio.
         */
        Titulo_Mal_Inscrito_No_Corresponde_Al_Predio,
        /** FMI que después de la intervención, no fue posible identificar en territorio y no se encuentra relacionado a ningún predio del la base catastral resultante de la actualización.
         */
        FMI_Sin_Correspondencia_Catastral
      );
    END ILC_NovedadFMI;

    CLASS ILC_TramitesDerechosTerritoriales =
      id_operacion_predio : TEXT*255;
      Entidad : MANDATORY Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.ILC_EntidadTipo;
      Fecha_Radicacion : INTERLIS.XMLDate;
      Numero_Radicacion : TEXT*20;
      Tramite : Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.ILC_TramiteTipo;
    END ILC_TramitesDerechosTerritoriales;

    CLASS Marcas =
      id_operacion_predio : TEXT*250;
      marcas : TEXT*250;
    END Marcas;

    /** Clase especializada de BaUnit, que describe la unidad administrativa básica para el caso de Colombia.
     * El predio es la unidad territorial legal propia de Catastro. Está formada por el terreno y puede o no tener construcciones asociadas.
     */
    CLASS ILC_Predio
    EXTENDS LADM_COL_V3_1.LADM_Nucleo.COL_UnidadAdministrativaBasica =
      id_operacion : TEXT*255;
      /** Corresponde al código del departamento al cual pertenece el predio. Es asignado por DIVIPOLA y tiene 2 dígitos.
       */
      Departamento : MANDATORY TEXT*2;
      /** Corresponde al código del municipio al cual pertenece el predio. Es asignado por DIVIPOLA y tiene 3 dígitos.
       */
      Municipio : MANDATORY TEXT*3;
      /** Circulo registral al que se encuentra inscrito el predio.
       */
      Codigo_ORIP : TEXT*4;
      /** Matricula inmobiliaria
       */
      Matricula_Inmobiliaria : 1 .. 9999999999;
      /** Área total del terreno resultante del levantamiento catastral.
       */
      Area_Catastral_Terreno : MANDATORY 0.00 .. 1000000000000000000000000.00 [LADM_COL_V3_1.m2];
      /** Nuevo Código numérico de treinta (30) dígitos, que se le asigna a cada predio y busca localizarlo inequívocamente en los documentos catastrales, según el modelo determinado por el Instituto Geográfico Agustín Codazzi.
       */
      Numero_Predial_Nacional : MANDATORY TEXT*30;
      /** Indica el tipo predio para efectos catastrales.
       */
      Tipo (EXTENDED) : MANDATORY Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.ILC_PredioTipo;
      /** Corresponde a una caracterización temática de las condiciones de predio que se está censando relacionada a aspectos físicos y jurídicos.
       */
      Condicion_Predio : MANDATORY Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.ILC_CondicionPredioTipo;
      /** Es la clasificación para fines estadísticos y económicos que se da a cada inmueble en su conjunto–terreno, construcciones o edificaciones-, en el momento de la identificación predial de conformidad con la actividad predominante que en él se desarrolle.
       */
      Destinacion_Economica : MANDATORY Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.ILC_DestinacionEconomicaTipo;
      /** Área consignada en el certificado de tradición y  libertad, en la descripción de cabida y linderos o en los titúlos registrados, expresada en metros cuadrados.
       */
      Area_Registral_M2 : 0.00 .. 1000000000000000000000000.00 [LADM_COL_V3_1.m2];
      Npn_Matriz : TEXT*255;
      Coeficiente_Copropiedad : 0 .. 100;
      Area_Total_Terreno_Ph_Cond_Matriz : 0.00 .. 1000000000000000000000000.00;
      Total_Unidades_Privadas : 1 .. 150;
      Numero_Torres : 1 .. 150;
      Area_Total_Construida_Comun_Ph_Cond_Matriz : 0.00 .. 1000000000000000000000000.00;
      Area_Total_Construida_Privada_Ph_Cond_Matriz : 0.00 .. 1000000000000000000000000.00;
      Area_Total_Construida_Ph_Cond_Matriz : 0.00 .. 1000000000000000000000000.00;
      Area_Total_Terreno_Comun_Ph_Cond_Matriz : 0.00 .. 1000000000000000000000000.00;
      Area_Total_Terreno_Privada_Ph_Cond_Matriz : 0.00 .. 1000000000000000000000000.00;
      Nombre_Predio : 0.00 .. 1000000000000000000000000.00;
      Tipo_Direccion : TEXT*255;
      Clase_Via_Principal : TEXT*255;
      Valor_Via_Principal : TEXT*255;
      Letra_Via_Principal : TEXT*28;
      Sector_Ciudad : TEXT*255;
      Valor_Via_Generadora : TEXT*255;
      Letra_Via_Generadora : TEXT*255;
      Sector_Predio : TEXT*255;
      Numero_Predio : NUMERIC;
      Complemento : TEXT*255;
      /** Registro asociado a la anotación en antiguo sistema de registro. (Libros)
       */
      Referencia_Registral_Sistema_Antiguo : Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.Interno_Levantamiento_Catastral.ExtReferenciaRegistralSistemaAntiguo;
      Tipo_Derecho : MANDATORY Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.ILC_DerechoCatastralTipo;
      Posesion_Ancestral_Y_O_Tradicional : BOOLEAN;
      Fecha_Inicio_Tenencia : INTERLIS.XMLDateTime;
      Descripcion : TEXT*255;
      Localizacion : ISO19107_PLANAS_V3_0.GM_MultiSurface3D;
      UNIQUE Numero_Predial_Nacional; 
    END ILC_Predio;

    ASSOCIATION cr_fuente_lcfuenteEspacialindigena =
      cr_fuenteespacial -- {0..*} ILC_FuentesEspacialesEtnicas;
      ilc_fuentesespacialesindigenas -- {0..*} CR_FuenteEspacial;
    END cr_fuente_lcfuenteEspacialindigena;

    ASSOCIATION cr_unidadconstruccion_caracteristicasunidadconstruccion =
      cr_caracteristicasunidadconstruccion -- {1} ILC_CaracteristicasUnidadConstruccion;
      cr_unidadconsutruccion -- {1..*} CR_UnidadConstruccion;
    END cr_unidadconstruccion_caracteristicasunidadconstruccion;

    ASSOCIATION ilc_informacion_adicional_contacto =
      ilc_contacto_visita -- {0..*} ILC_ContactoVisita;
      ilc_datos_adicionales -- {1} ILC_DatosAdicionalesLevantamientoCatastral;
    END ilc_informacion_adicional_contacto;

    ASSOCIATION ilc_interesado_identificacionetnica =
      lc_interesado -- {0..*} ILC_IdentificacionEtnica;
      lc_identificacionetnica -- {1} ILC_Interesado;
    END ilc_interesado_identificacionetnica;

    ASSOCIATION ilc_interesado_interesadocontacto =
      ilc_contacto -- {0..*} ILC_InteresadoContacto;
      cr_interesado -- {1} ILC_Interesado;
    END ilc_interesado_interesadocontacto;

    /** Relación entre unidades prediales y predios matrices bajo el régimen de propiedad horizontal y condominio.
     */
    ASSOCIATION CR_Predio_Copropiedad =
      /** Area privada de terreno matriz, adicional a la participación en el terreno común, según el coeficiente allí determinado.
       */
      unidad_predial -- {0..*} ILC_Predio;
      /** Unidades residenciales tales como edificios, conjuntos y tipos de urbanizaciones cerradas en las que se comparten áreas comunes y que tienen régimen de propiedad horizontal.
       */
      matriz -<> {0..1} ILC_Predio;
      /** Coeficiente de copropiedad de la unidad predial.
       */
      Coeficiente : MANDATORY 0.000000000000 .. 1.000000000000;
      /** En predios en condición de PH Unidad predial o Parque cementerio unidad predial, corresponde al área de terreno producto del coeficiente de copropiedad de la unidad de PH por el área catastral del predio PH matriz.  
       * 
       * En predios condominio unidad predial, corresponde al área obtenida de la suma (1) del área de la representación geométrica del terreno del predio privado y (2) el área común, calculada mediante el coeficiente de propiedad de la unidad de condominio por el área geográfica del terreno del predio matriz.
       */
      Area_Coeficiente : MANDATORY 0.00 .. 99999999999999.98 [LADM_COL_V3_1.m2];
    END CR_Predio_Copropiedad;

    /** Relación entre el Predio catastral y la clase información adicional, que permite determinar sobre qué predio se toman los datos específicos del predio para catastro multipropósito
     */
    ASSOCIATION ilc_informacion_adicional_predio =
      ilc_predio (EXTERNAL) -- {1} ILC_Predio;
      ilc_datos_adicionales -- {1} ILC_DatosAdicionalesLevantamientoCatastral;
    END ilc_informacion_adicional_predio;

    ASSOCIATION ilc_ph_predio =
      ilc_predio -- {1} ILC_Predio;
      cr_datos_ph -- {0..1} CR_DatosPHCondominio;
    END ilc_ph_predio;

    ASSOCIATION ilc_predio_informalidad =
      id_operacion_predio_formal -- {0..*} ILC_Predio;
      id_operacion_predio_informal -- {0..*} ILC_Predio;
    END ilc_predio_informalidad;

    ASSOCIATION lc_tramites_lc_predio =
      lc_predio -- {0..*} ILC_TramitesDerechosTerritoriales;
      lc_tramitesderechosterritoriales -- {1..*} ILC_Predio;
    END lc_tramites_lc_predio;

  END Interno_Levantamiento_Catastral;

END Modelo_Aplicacion_Interno_Levantamiento_Catastral_LADMCOL_V1_0.
