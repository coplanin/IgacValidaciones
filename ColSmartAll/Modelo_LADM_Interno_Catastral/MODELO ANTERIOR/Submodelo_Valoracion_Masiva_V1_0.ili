  /** Este Topic agrupa a las clases específicas del avalúo, así como sus atributos.
   */
  TOPIC Avaluos =
    OID AS INTERLIS.UUIDOID;

    /** Clase los valores por M2 de construcción según el uso y puntaje de calificación de construcción.
     */
    CLASS VM_TablaCalificacionConstruccion =
      /** Valor por metros cuadrado de construcción clasificando por uso y puntaje de las unidades de construcción.
       */
      Valor_M2_Construccion : LADM_COL_V3_1.LADM_Nucleo.Peso;
      /** Es la actividad que se desarrolla en una unidad de construcción.
       */
      Uso : Submodelo_Avaluos_V2_0.VM_UsoUConsTipo;
      /** Puntaje de las edificaciones de acuerdo a sus características constructivas.
       */
      Puntuacion : TEXT*20;
    END VM_TablaCalificacionConstruccion;

    /** Clase que relaciona las tipologías de las construcciones con su respectivo valor por M2 de construcción.
     */
    CLASS VM_TipologiaConstruccion =
      /** Se refiere a la zonificación del municipio de acuerdo con las clasificaciones de los inmuebles según su actividad residencial, comercial e industrial.
       */
      Tipo_Tipologia : Submodelo_Avaluos_V2_0.VM_TipologiaTipo;
      /** Valor unitario en pesos del tipo de tipología constructiva.
       */
      Valor_Unitario : LADM_COL_V3_1.LADM_Nucleo.Peso;
      Tipo_Anexo : Submodelo_Avaluos_V2_0.VM_AnexoTipo;
    END VM_TipologiaConstruccion;

    /** Las ZHF Son espacios geográficos con características similares en cuanto a vías, uso actual del suelo, norma de uso del suelo, áreas homogéneas de tierra, disponibilidad de aguas superficiales permanentes y otras variables que permitan diferenciar estas áreas de las adyacentes.
     */
    CLASS VM_ZonaHomogeneaFisicaRural =
      /** Código único del registro de la instancia de la zona geoeconómica.
       */
      Codigo : MANDATORY TEXT*20;
      /** Identificador de la zona homogénea física rural.
       */
      Codigo_Zona_Fisica : MANDATORY TEXT*20;
      /** Espacios de la superficie terrestre que presentan características o cualidades similares en cuanto a las condiciones de clima, relieve, material litológico superficial o depósitos superficiales y de las características internas de los suelos, condiciones que expresan la capacidad productiva de las tierras; ello se indica mediante un valor numérico denominado Valor Potencial (VP).
       */
      Area_Homogenea_Tierra : TEXT*15;
      /** En el estudio de esta variable se analiza la existencia de fuentes de aguas superficiales permanentes y su clasificación, con el fin de zonificar de acuerdo con la posibilidad de aprovechamiento para el uso actual que se da al suelo.
       */
      Disponibilidad_Agua : Submodelo_Avaluos_V2_0.VM_DisponibilidadAguaTipo;
      /** Se refiere a la existencia y calidad de las vías, analizada a nivel de zona. A esta condición se asocia la existencia de otras obras de ingeniería, la instalación de redes de servicios públicos, etc.
       */
      Influencia_Vial : Submodelo_Avaluos_V2_0.VM_InfluenciaVialRuralTipo;
      /** Actividad que se desarrolla en un determinado espacio geográfico en el momento de la elaboración del estudio de zonas homogéneas físicas.
       */
      Uso_Actual_Suelo : MANDATORY Submodelo_Avaluos_V2_0.VM_UsoSueloRuralTipo;
      /** Indicación legal sobre la actividad que se puede desarrollar en un determinado espacio geográfico con lo planificado y reglamentado por la respectiva autoridad de la unidad orgánica catastral.
       */
      Norma_Uso_Suelo : TEXT*250;
      /** Es posible tener en cuenta otras variables de acuerdo con condiciones especiales y que pueden llegar a incidir en la determinación de los valores unitarios del terreno, como por ejemplo: Riesgos geológicos representativos, amenazas naturales que inciden en la zona, reportadas por autoridades competentes, Nivel Freático, % de arcilla en el suelo, entre otras
       */
      Otras_Variables_R : TEXT*20;
      /** Vigencia del estudio de zonas homogéneas físicas rurales.
       */
      Vigencia : MANDATORY INTERLIS.XMLDate;
      /** Definición de la geometría espacial georreferenciada de la zona homogénea física, delimitada mediante un polígono que contiene sus linderos y la superficie ocupada.
       */
      Geometria : MANDATORY ISO19107_PLANAS_V3_0.GM_MultiSurface3D;
    END VM_ZonaHomogeneaFisicaRural;

    /** Las ZHF Son espacios geográficos con características similares en cuanto a vías, topografía, servicios públicos, uso actual del suelo tipificado, norma de uso del suelo y otras variables que permitan diferenciar estas áreas de las adyacentes.
     */
    CLASS VM_ZonaHomogeneaFisicaUrbana =
      /** Código único del registro de la instancia de Zona Homegenea FÍsica.
       */
      Codigo : MANDATORY TEXT*20;
      /** Identificador de la zona homogénea física urbana.
       */
      Codigo_Zona_Fisica : MANDATORY TEXT*20;
      /** Características naturales relacionadas con el relieve que permitan o dificulten el desarrollo de las construcciones, dentro de esta variable deben considerarse factores tales como zonas anegadizas o inundables, las discontinuidades físicas tales como taludes, caños y ríos, altura por encima de la cota de servicios, áreas potenciales de derrumbe, por inclinación o presencia de drenajes superficiales.
       */
      Topografia : MANDATORY Submodelo_Avaluos_V2_0.VM_TopografiaZonaTipo;
      /** Se refiere a la existencia y calidad de las vías, analizada a nivel de zona. A esta condición se asocia la existencia de otras obras de ingeniería, la instalación de redes de servicios públicos, etc.
       */
      Influencia_Vial : MANDATORY Submodelo_Avaluos_V2_0.VM_InfluenciaVialUrbanaTipo;
      /** Se prestan con la finalidad específica de satisfacer las necesidades esenciales de las personas. Para efectos de la presente metodología se consideran servicios públicos básicos: Acueducto, Alcantarillado y Energía Eléctrica.
       */
      Servicios_Publicos : MANDATORY Submodelo_Avaluos_V2_0.VM_ServiciosPublicosTipo;
      /** Caracterización tanto del uso actual del suelo como la tipificación de las construcciones, a través de la identificación de la actividad que se desarrolla en un determinado espacio geográfico y a su vez, el tipo de construcciones existentes, teniendo en cuenta ciertas características observables de las mismas, para el momento de la elaboración del estudio de zonas homogéneas físicas.
       */
      Uso_Actual_Tipificado : MANDATORY Submodelo_Avaluos_V2_0.VM_UsoSueloTipificadoUrbanoTipo;
      /** Indicación legal sobre la actividad que se puede desarrollar en un determinado espacio geográfico con lo planificado y reglamentado por la respectiva autoridad de la unidad orgánica catastral.
       */
      Norma_Uso_Suelo : TEXT*250;
      /** Es posible tener en cuenta otras variables de acuerdo con condiciones especiales y que pueden llegar a incidir en la determinación de los valores unitarios del terreno, como por ejemplo: Riesgos geológicos representativos, amenazas naturales que inciden en la zona, reportadas por autoridades competentes, Nivel Freático, % de arcilla en el suelo, entre otras
       */
      Otras_Variables_U : TEXT*20;
      /** Vigencia del estudio de zonas homogéneas físicas urbanas.
       */
      Vigencia : MANDATORY INTERLIS.XMLDate;
      /** Definición de la geometría espacial georreferenciada de la zona homogénea física, delimitada mediante un polígono que contiene sus linderos y la superficie ocupada.
       */
      Geometria : MANDATORY ISO19107_PLANAS_V3_0.GM_MultiSurface3D;
    END VM_ZonaHomogeneaFisicaUrbana;

    /** Las ZHG son los espacios geográficos determinados a partir de Zonas Homogéneas Físicas con valores unitarios similares en cuanto a su precio, según las condiciones del mercado inmobiliario.
     */
    CLASS VM_ZonaHomogeneaGeoeconomicaRural =
      /** Código único del registro de la instancia de la zona geoeconómica.
       */
      Codigo : MANDATORY TEXT*20;
      /** Identificador de la zona homogénea geoeconómica rural.
       */
      Codigo_Zona_Geoeconomica : MANDATORY TEXT*20;
      /** Valor económico de la zona homogénea que representa la instancia de la clase.
       */
      Valor_Hectarea : MANDATORY LADM_COL_V3_1.LADM_Nucleo.Peso;
      /** Códigos de las zonas homogéneas físicas rurales que componen la zona homogénea geoeconómica rural.
       */
      Subzona_Fisica : TEXT*20;
      /** Vigencia del estudio de zonas homogéneas geoeconómicas rurales.
       */
      Vigencia : INTERLIS.XMLDate;
