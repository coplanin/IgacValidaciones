    /** Estructura que permite identificar las inconsistencias que puede tener el folio de matrícula inmobiliaria producto de la dinámica inmobiliaria identificada en el levantamiento catastral.
     */
    STRUCTURE ILC_NovedadFMI =
      /** Es el consecutivo que se asigna a cada oficina de registro de instrumentos públicos.
       */
      Codigo_ORIP : MANDATORY TEXT*4;
      /** Es el consecutivo que se asigna a cada predio jurídico abierto en la ORIP.
       */
      Numero_FMI : MANDATORY TEXT*80;
      /** Relaciona todas aquellas diferencias e inexactitudes relativas al registro de los predios respecto de la información jurídica vigente.
       */
      Tipo_NovedadFMI : (
        /** Cuando el estado del FMI del predio en estudio es CERRADO ante la SNR, pero pese a haberse realizado varios negocios jurídicos sobre un predio (ventas, donaciones, segregaciones, etc.) se identifica que este posee un área de remanente, lo que le permite continuar con su existencia jurídica y su estado debe cambiar a ABIERTO.
         */
        FMI_Debe_Estar_Abierto,
        /** Cuando el estado del FMI del predio en estudio es ABIERTO ante la SNR, pero el folio ha sido cancelado por orden de autoridad  judicial y/o administrativa, o se agote su área por englobes, desenglobes o cualquier otro negocio jurídico  y su estado debe cambiar a CERRADO.
         */
        FMI_Debe_Estar_Cerrado,
        /** Cuando no exista relación entre el predio segregado y el que se enuncia.
         */
        FMI_No_Es_Segregado_Del_Que_Se_Enuncia,
        /** Cuando se ha aperturado dos o mas FMI sobre un mismo predio, en un mismo circulo registral o círculos registrales diferentes.
         */
        FMI_Duplicado_Sobre_El_Mismo_Bien_Inmueble,
        /** Cuando se presente una inconsistencia en el código registral con el que se registró el acto jurídico
         */
        FMI_Con_Error_En_La_Identificacion_De_Naturaleza_Juridica_Del_Acto,
        /** Cuando se encuentra un titulo inscrito que no corresponde al predio objeto de estudio.
         */
        Titulo_Mal_Inscrito_No_Corresponde_Al_Predio,
        /** FMI que después de la intervención, no fue posible identificar en territorio y no se encuentra relacionado a ningún predio del la base catastral resultante de la actualización.
         */
        FMI_Sin_Correspondencia_Catastral
      );
    END ILC_NovedadFMI;

    /** Clase especializada de BaUnit, que describe la unidad administrativa básica para el caso de Colombia.
     * El predio es la unidad territorial legal propia de Catastro. Está formada por el terreno y puede o no tener construcciones asociadas.
