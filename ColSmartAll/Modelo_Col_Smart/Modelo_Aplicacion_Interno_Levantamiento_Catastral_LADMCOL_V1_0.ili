      Matricula_Inmobiliaria : 1 .. 9999999999;
      /** Área total del terreno resultante del levantamiento catastral.
       */
      Area_Catastral_Terreno : MANDATORY 0.00 .. 1000000000000000000000000.00;
      /** Nuevo Código numérico de treinta (30) dígitos, que se le asigna a cada predio y busca localizarlo inequívocamente en los documentos catastrales, según el modelo determinado por el Instituto Geográfico Agustín Codazzi.
       */
      Numero_Predial_Nacional : MANDATORY TEXT*30;
