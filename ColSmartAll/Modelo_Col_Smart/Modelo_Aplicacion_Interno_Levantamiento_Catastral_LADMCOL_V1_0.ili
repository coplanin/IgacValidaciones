        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "Mejora" a "NPH"
         */
        Cambio_Numero_Predial_Mejora_a_NPH,
        /** Se asigna esta novedad al predio que cambia  su "Numero Predial Nacional" al pasar de "Condicion_predio" de "NPH" a "Informal"
         */
        Cambio_Numero_Predial_NPH_a_Informal,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "NPH" a "Condominio"
         */
        Cambio_Numero_Predial_NPH_a_Condominio,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "NPH" a "PH"
         */
        Cambio_Numero_Predial_NPH_a_PH,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "NPH" a "Via"
         */
        Cambio_Numero_Predial_NPH_a_VIA,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "NPH" a "Bien_Uso_Publico"
         */
        Cambio_Numero_Predial_NPH_a_Bien_Uso_Publico,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de "PH" a "Condominio"
         */
        Cambio_Numero_Predial_PH_a_Condominio,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de  "Condominio" a "PH"
         */
        Cambio_Numero_Predial_Condominio_a_PH,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de "Condicion_predio" de  "Condominio" a "NPH"
         */
        Cambio_Numero_Predial_Condominio_a_NPH,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de estar inscrito catastralmente en la zona urbana o corregimiento, a la zona rural.
         */
        Cambio_Numero_Predial_Urbano_a_Rural,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de estar inscrito catastralmente en la zona rural, a la zona urbana o corregimiento
         */
        Cambio_Numero_Predial_Rural_a_Urbano,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de estar inscrito en una manzana catastral a otra, dentro de la zona urbana o corregimiento
         */
        Cambio_Numero_Predial_Manzana_a_Manzana,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de estar inscrito en una vereda catastral a otra, dentro de la zona rural
         */
        Cambio_Numero_Predial_Vereda_a_Vereda,
        /** Se asigna esta novedad al predio que cambia su "Numero Predial Nacional" al pasar de estar inscrito en una municipio a otro.
         */
        Cambio_Numero_Predial_Municipio_a_Municipio,
        /** Se asigna esta novedad a los predios con "numero provisional" que se incluyen en la base catastral y que no se originan a partir de un englobe, desenglobe o cambio de numero predial.
         */
        Predio_Nuevo
      );
      ilc_predio : MANDATORY REFERENCE TO ILC_Predio;
    END ILC_EstructuraNovedadNumeroPredial;

